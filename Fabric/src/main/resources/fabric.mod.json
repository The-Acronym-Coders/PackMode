{
    "schemaVersion": 1,
    "id": "packmode",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://github.com/The-Acronym-Coders/PackMode",
        "issues": "${issues_url}",
        "sources": "https://github.com/The-Acronym-Coders/PackMode"
    },
    "license": "${license}",
    "icon": "assets/packmode/logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.teamacronymcoders.packmode.PackModeFabric"
        ]
    },
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "crafttweaker"
    }
}
  